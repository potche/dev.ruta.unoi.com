<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UNO\EvaluacionesBundle\Entity\Optionapplication" table="OptionApplication">
    <id name="optionApplicationId" type="integer" column="optionApplicationId">
      <generator strategy="AUTO"/>
    </id>
    <field name="nameOptionApplication" type="string" column="nameOptionApplication" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descOptionApplication" type="string" column="descOptionApplication" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iconOptionApplication" type="string" column="iconOptionApplication" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ruteOptionApplication" type="string" column="ruteOptionApplication" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusOptionApplication" type="boolean" column="statusOptionApplication" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
