<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UNO\EvaluacionesBundle\Entity\ObservationAnswerHistory" table="ObservationAnswerHistory">
    <id name="observationAnswerId" type="integer" column="observationAnswerId">
      <generator strategy="NONE"/>
    </id>
    <field name="questionId" type="integer" column="questionId">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="answer" type="boolean" column="answer">
    </field>
    <field name="comment" type="string" column="comment" length="300">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="personId" type="integer" column="personId">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateRecord" type="datetime" column="dateRecord">
    </field>
    <field name="observationId" type="integer" column="observationId">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
