<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UNO\EvaluacionesBundle\Entity\Person" table="Person">
    <id name="personid" type="integer" column="personId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="user" type="string" column="user" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="string" column="surname" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gender" type="string" column="gender" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthday" type="string" column="birthDay" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthmonth" type="string" column="birthMonth" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="birthyear" type="string" column="birthYear" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="languageid" type="integer" column="languageId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="languagecode" type="string" column="languageCode" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="string" column="language" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timezone" type="string" column="timeZone" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="admin" type="boolean" column="admin" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
