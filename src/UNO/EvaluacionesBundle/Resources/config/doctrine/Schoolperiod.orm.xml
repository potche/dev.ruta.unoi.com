<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UNO\EvaluacionesBundle\Entity\Schoolperiod" table="SchoolPeriod">
    <id name="schoolperiodid" type="integer" column="schoolPeriodId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="schoolperiodcode" type="string" column="schoolPeriodCode" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="schoolperiod" type="string" column="schoolPeriod" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="schoolperiodactive" type="string" column="schoolPeriodActive" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
